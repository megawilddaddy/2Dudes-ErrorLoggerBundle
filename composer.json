{
    "name": "twodudes/error-logger-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 production error logger. Logs notices, warnings, exceptions and fatal exceptions. Groups similar errors together and displays additional server information.",
    "keywords": ["error logger", "error", "symfony2 error", "production", "logger"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vasily Sokolov",
            "email": "megawilddaddy@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "TwoDudes\\ErrorLoggerBundle\\": ""
        }
    },
    "target-dir": "TwoDudes/ErrorLoggerBundle",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "ext-pdo": "*"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type":"package",
            "package": {
                "version":"dev-master",
                "name":"2Dudes-ErrorLoggerBundle",
                "source": {
                    "url":"https://github.com/megawilddaddy/2Dudes-ErrorLoggerBundle.git",
                    "type":"git",
                    "reference":"master"
                }
            }
        }
    ]
}